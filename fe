def connection(url='http://www.google.com/', timeout=5):
    try:
        verficateur_connexion = requests.get(url, timeout=timeout)
        verficateur_connexion.raise_for_status()
	Street("\033[1;91m[\033[1;92mâ—â—\033[1;91m]          \033[1;97m\033[1;92mVerification de la connexion      \033[1;91m[\033[1;92mâ—â—\033[1;91m]")
        auto("\t\t\033[1;92mVous Ãªtes connectÃ© Ã  Internet\n")
	raw_input("\t\t\033[1;91m[[38;5;24mSuivant\033[1;91m]")
        return True
    except requests.HTTPError as e:
        print("La vÃ©rification de la connexion Internet a Ã©chouÃ©, code d'Ã©tat {0}.".format(e.response.status_code))
    except requests.ConnectionError:
        auto("\t\t\033[1;91mVous n'Ãªtes pas connectÃ© Ã  Internet.")
    return False
