# -Automatisation-#
def Street(z):
    for e in z + '\n':
        sys.stdout.write(e)
        sys.stdout.flush()
        time.sleep(0.009)
####################################################################################################		
def auto(e):
	for z in e + '\n':
		sys.stdout.write(z)
		sys.stdout.flush()
		time.sleep(0.009)	
